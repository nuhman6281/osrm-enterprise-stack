{
  "name": "OSRM Enterprise Maps",
  "short_name": "OSRM Maps",
  "description": "Enterprise-grade routing and navigation powered by OSRM",
  "version": "2.0.0",
  "start_url": "/",
  "display": "standalone",
  "orientation": "portrait-primary",
  "theme_color": "#667eea",
  "background_color": "#ffffff",
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
  "categories": ["navigation", "travel", "utilities"],
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml"
    }
  ],
  "shortcuts": [
    {
      "name": "Find Route",
      "short_name": "Route",
      "description": "Calculate a route between two points",
      "url": "/?action=route",
      "icons": [
        {
          "src": "icons/route-icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "My Location",
      "short_name": "Location",
      "description": "Show my current location",
      "url": "/?action=locate",
      "icons": [
        {
          "src": "icons/location-icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Nearby Places",
      "short_name": "Nearby",
      "description": "Find nearby points of interest",
      "url": "/?action=nearby",
      "icons": [
        {
          "src": "icons/nearby-icon-96x96.png",
          "sizes": "96x96",
          "type": "image/png"
        }
      ]
    }
  ],
  "screenshots": [
    {
      "src": "screenshots/desktop-1.png",
      "sizes": "1280x720",
      "type": "image/png",
      "form_factor": "wide",
      "label": "OSRM Enterprise Maps - Desktop View"
    },
    {
      "src": "screenshots/mobile-1.png",
      "sizes": "375x812",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "OSRM Enterprise Maps - Mobile View"
    }
  ],
  "features": [
    "routing",
    "geocoding",
    "offline-maps",
    "turn-by-turn-navigation",
    "multiple-transport-modes",
    "isochrones",
    "trip-optimization"
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "handle_links": "preferred",
  "capture_links": "existing-client-navigate",
  "protocol_handlers": [
    {
      "protocol": "geo",
      "url": "/?geo=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/import",
      "accept": {
        "application/gpx+xml": [".gpx"],
        "application/json": [".geojson"],
        "text/csv": [".csv"]
      }
    }
  ]
}
